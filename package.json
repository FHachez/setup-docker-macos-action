{
  "name": "setup-docker-action",
  "version": "1.0.0",
  "description": "This action installs Docker on a macOS runner through [Colima], [Lima-VM], and [Homebrew].",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format:check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build lib/index.js --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglascamata/setup-docker-action.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/douglascamata/setup-docker-action/issues"
  },
  "homepage": "https://github.com/douglascamata/setup-docker-action#readme",
  "dependencies": {
    "@actions/cache": "^3.0.2",
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.3",
    "@actions/glob": "^0.3.0",
    "@actions/io": "^1.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.21.0",
    "eslint-plugin-github": "^4.3.7",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
